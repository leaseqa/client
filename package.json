{
  "name": "leaseqa-monorepo",
  "version": "0.1.0",
  "private": true,
  "packageManager": "npm@10.9.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "npm --workspace=@leaseqa/web run dev",
    "build": "npm --workspace=@leaseqa/web run build",
    "lint": "npm --workspace=@leaseqa/web run lint",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\""
  },
  "engines": {
    "node": ">=25.1.0"
  },
  "devDependencies": {
    "prettier": "^3.3.3"
  },
  "dependencies": {
    "react-redux": "^9.2.0"
  }
}
